# Пример конфигурационного файла
# Скопируйте этот файл в .env и заполните своими данными

# API ключ OpenRouter
OPENROUTER_API_KEY=ваш_ключ_openrouter_здесь

# Настройки базы данных PostgreSQL
DB_HOST=localhost
DB_NAME=rag_chatbot
DB_USER=postgres
DB_PASSWORD=ваш_пароль_postgres
DB_PORT=5432

# Вы можете оставить пустым для тестирования
# OPENROUTER_API_KEY=